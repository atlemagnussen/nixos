{
  "ReverseProxy": {
    "Routes": {
      "routeRadarr": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "Radarr",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts" : ["radarr.example.com"]
        }
      },
      "routeSonarr": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "Sonarr",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts" : ["sonarr.example.com"]
        }
      }
    },
    "Clusters": {
      "Radarr": {
        "Destinations": {
          "radarr": {
            "Address": "http://host.containers.internal:7878/"
          }
        }
      },
      "Sonarr": {
        "Destinations": {
          "sonarr": {
            "Address": "http://host.containers.internal:8989/"
          }
        }
      }
    }
  }
}