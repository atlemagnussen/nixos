;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns1.atle.guru. admin.atle.guru. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL

; name servers - NS records
      IN      NS      ns1.atle.guru.

; PTR Records
1   IN      PTR     ns1.atle.guru.    ; 192.168.1.1
3   IN      PTR     atle.guru.        ; 192.168.1.3
3   IN      PTR     www.atle.guru.    ; 192.168.1.3
19  IN      PTR     nuc.atle.guru.    ; 192.168.1.19
21  IN      PTR     srv.atle.guru.    ; 192.168.1.21